{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "^_nuxt/**/*.*",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000, stale-while-revalidate"
      } ]
    } ]
  }
}
